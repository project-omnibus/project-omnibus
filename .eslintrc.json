{
  "extends": ["standard", "standard-react"],
  "env": {
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "rules": {
    "semi": [2, "always"],
    "react/prop-types": 0,
    "no-unused-vars": ["error", { "vars": "all", "args": "none", "varsIgnorePattern": "should" }]
  }
}
