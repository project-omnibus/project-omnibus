  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "body-parser": "1.18.3",
    "concurrently": "4.0.1",
    "corenlp": "^1.4.3",
    "cross-env": "5.2.0",
    "es6-promise": "^4.2.5",
    "express": "4.16.4",
    "heroku": "^7.19.4",
    "http-proxy-middleware": "0.19.1",
    "isomorphic-fetch": "^2.2.1",
    "log4js": "3.0.6",
    "lower-case": "^1.1.4",
    "natural": "^0.6.2",
    "node-uuid": "1.4.8",
    "nodemon": "1.18.6",
    "pg": "^7.7.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router": "^4.3.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "request": "2.88.0",
    "request-promise": "4.2.2",
    "underscore": "1.8.3",
    "yarn": "1.12.3"
  },
  "scripts": {
    "server": "cd server && nodemon src/server.js",
    "start:dev": "concurrently --kill-others-on-fail  \"npm run build:watch:server\" \"npm run build:watch:client\" \"nodemon ./build/server/start.js\"",
    "style": "eslint shared server client",
    "style-fix": "eslint --fix shared server client",
    "build:server": "babel ./server/src/start.js -d build/server/",
    "build:watch:server": "babel ./server/src/start.js -d build/server/ --watch",
    "build:client": "webpack --config ./webpack.config.js/",
    "build:watch:client": "webpack --config ./webpack.config.js/ --watch",
    "build:prod": "npm run build:server && npm run build:client",
    "start:dev:client": "webpack-dev-server",
    "build:production": "./node_modules/.bin/webpack --mode production",
    "build:dev": "./node_modules/.bin/webpack --mode development",
    "dev": "nodemon server/src/start.js",
    "start": "nodemon server/src/start.js",
    "prestart": "npm run build:dev",
    "test": "jest --runInBand --coverage --detectOpenHandles --forceExit",
    "heroku-postbuild": "webpack --mode development"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-transform-async-to-generator": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "css-loader": "^2.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.6.0",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-react": "7.0.2",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-standard": "4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "istanbul": "0.4.5",
    "jest": "^23.6.0",
    "mocha": "5.2.0",
    "nock": "10.0.2",
    "nyc": "13.1.0",
    "should": "^13.2.3",
    "sinon": "^7.2.2",
    "supertest": "3.3.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
